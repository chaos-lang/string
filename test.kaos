import string

// String Operations
print string.upper("hello world")
print string.lower("HeLlO WoRLd")
print string.capitalize("hello world")
print string.capitalize("")
print string.concat("hello", " world")
print string.split("A quick brown fox jumps over the lazy dog", " ")
list a = ["foo", "bar", "baz"]
print string.join(a, " ")
print string.join(a, "")

// Searching & Replacing
print string.search("hello world", "world")
print string.search("hello world", "friend")
print string.replace("hello world", "world", "friend")

// Information Functions
print string.length("hello world")
print string.length("")
print string.is_empty("hello world")
print string.is_empty("")
print string.is_numeric("01234")
print string.is_numeric("01234\t")
print string.is_numeric("01234\n")
print string.is_numeric("01234x")
print string.is_numeric(" 01234")
print string.is_numeric(" ")
print string.is_numeric("")
print string.is_alpha("hello world")
print string.is_alpha("hello world\t")
print string.is_alpha("he11o w0rld")
print string.is_alpha("he11o w0rld\n")
print string.is_alpha(" ")
print string.is_alpha("")
print string.is_alnum("he11o w0rld")
print string.is_alnum("he11o w0rld\t")
print string.is_alnum("he11o w0rld\n")
print string.is_alnum(" ")
print string.is_alnum("")
print string.is_space("he11o w0rld\t")
print string.is_space("a")
print string.is_space("1")
print string.is_space("\n")
print string.is_space(" ")
print string.is_space("")
print string.is_lower("hello world")
print string.is_lower("hello world 01234")
print string.is_lower("HELLO WORLD")
print string.is_lower("HeLlO WoRLd")
print string.is_lower(" ")
